char USER_PROGRAMS[] = "ls\0pong\0rainbow\0snake_paint\0";
int USER_PROGRAM_COUNT = 4;

int ls();
int pong();
int rainbow();
int snake_paint();

int call_user_program(int program) {
	if (program == 0) {
		ls();
	}
	else if (program == 1) {
		pong();
	}
	else if (program == 2) {
		rainbow();
	}
	else if (program == 3) {
		snake_paint();
	}
	else {}
}

#include ls.cmm
#include pong.cmm
#include rainbow.cmm
#include snake_paint.cmm

