char USER_PROGRAMS[] = "ls\0pong\0snake_paint\0";
int USER_PROGRAM_COUNT = 3;

int ls();
int pong();
int snake_paint();

int call_user_program(int program) {
	if (program == 0) {
		ls();
	}
	else if (program == 1) {
		pong();
	}
	else if (program == 2) {
		snake_paint();
	}
	else {}
}

